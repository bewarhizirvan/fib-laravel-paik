{
  "name": "first-iraqi-bank/fib-laravel-payment-sdk",
  "description": "Laravel SDK for integrating with the FIB payment system, enabling user authentication and payment transactions.",
  "version": "1.3.0",
  "type": "library",
  "keywords": [
    "sdk",
    "fib",
    "payment",
    "integration"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gateway ICT",
      "email": "info@the-gw.com",
      "homepage": "https://the-gw.com/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/contracts": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/http": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0",
    "phpunit/phpunit": "^8.5 || ^9.5 || ^10.0",
    "laravel/legacy-factories": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "FirstIraqiBank\\FIBPaymentSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FirstIraqiBank\\FIBPaymentSDK\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "FirstIraqiBank\\FIBPaymentSDK\\FIBPaymentServiceProvider"
      ]
    }
  }
}
